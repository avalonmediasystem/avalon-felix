# where to mount the OAI-PMH server below org.opencastproject.server.url
# please make sure that the path configured is accessible without any login (see security.xml)
mount-path=/oaipmh

# plain text name of the repository
repository-name=Matterhorn OAI-PMH repository

# email address of the repository's administrator
admin-email=admin@localhost

# maximum number of items returned per request
result-limit=100